<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les analyses IA -->
        <record id="seq_supplier_ai_analysis" model="ir.sequence">
            <field name="name">Analyses IA Fournisseurs</field>
            <field name="code">supplier.ai.analysis</field>
            <field name="prefix">IA-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les sessions chatbot -->
        <record id="seq_supplier_ai_chatbot" model="ir.sequence">
            <field name="name">Sessions Chatbot IA</field>
            <field name="code">supplier.ai.chatbot</field>
            <field name="prefix">CHAT-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>